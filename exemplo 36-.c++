#include <stdio.h>

int main() {
    int a = 5;// Declaração da variável 'a' e atribuição do valor 5.
    int b = 3;// Declaração da variável 'b' e atribuição do valor 3.
    int result;// Declaração da variável 'result'.

    result = a - b;

    printf("O resultado da subtração é: %d\n", result);

    return 0;
}